<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

	<bean class="com.GroupTree.action.HelloWorld">
		<property name="path" value="OK"></property>
	</bean>
	
	<bean name="login" scope="prototype" class="com.GroupTree.action.Login">
	</bean>
	
	<bean name="generateGroupTree" scope="prototype" class="com.GroupTree.action.GenerateGroupTree">
		<property name="groupDAO" ref="testGroupDAO"></property>
	</bean>
	
	<bean name="getGroupOrUserInfo" scope="prototype" class="com.GroupTree.action.GetGroupOrUserInfo">
	</bean>
	
	
	<bean name="testGroupDAO" class="Junit.Test.TestGroupDAOImpl">
	</bean>
</beans>